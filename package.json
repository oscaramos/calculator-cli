  "keywords": [
    "oclif"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "calculator",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "oscaramos/calculator-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
